{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "semicolon": [true, "always"],
    "max-line-length": [true, {
      "limit": 130,
      "ignore-pattern": "^import |^export {(.*?)}"
    }],
    "quotemark": [true, "single"]
  },
  "rulesDirectory": []
}